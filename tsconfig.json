{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "strictNullChecks": true,
    "allowJs": true,
    "types": ["node", "astro/client"],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}